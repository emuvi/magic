{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
    "initialCols": 99,
    "initialRows": 27,
    "profiles": 
    {
        "defaults": 
        {
            "colorScheme": "Vintage",
            "font": 
            {
                "face": "Fira Code",
                "size": 14
            }
        },
        "list": 
        [
            {
                "colorScheme": "Vintage",
                "commandline": "cmd.exe",
                "cursorShape": "emptyBox",
                "font": 
                {
                    "face": "Fira Code",
                    "size": 14,
