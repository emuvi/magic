{
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "cSpell.language": "en,pt,pt_BR",
    "cSpell.userWords": [
        "abdesk",
        "admister",
        "adpeople",
        "adsales",
        "archius",
        "authed",
        "automagic",
        "byteranges",
        "cfgs",
        "classpath",
        "cmds",
        "codehaus",
        "darcula",
        "dbcp",
        "delety",
        "dics",
        "eorm",
        "esql",
        "fileset",
        "filesets",
        "firebirdsql",
        "flatlaf",
        "formdev",
        "gson",
        "hsql",
        "hsqldb",
        "inkscape",
        "instanceof",
        "jarch",
        "jdbc",
        "jeemuvi",
        "jupiter",
        "moodle",
        "mysql",
        "pointel",
        "postgre",
        "postgresql",
        "pycache",
        "qindred",
        "qine",
        "qined",
        "qinpel",
        "Resizer",
        "servlet",
        "sqlite",
        "urvs",
        "xerial"
    ],
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "never",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.detectIndentation": true,
    "editor.fontFamily": "Victor Mono Semibold, Victor Mono, Fira Code, Hack, Source Code Pro, Courier New, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.fontWeight": "500",
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.guides.bracketPairs": "active",
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 36,
    "editor.minimap.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.parameterHints.enabled": false,
    "editor.pasteAs.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 14,
    "editor.suggest.showReferences": false,
    "editor.suggestOnTriggerCharacters": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.tabSize": 4,
    "editor.tokenColorCustomizations": {
        "keywords": {
            "fontStyle": "bold"
        }
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrap": "off",
    "editor.wordWrapColumn": 108,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.giz": "groovy",
        "*.gvy": "groovy"
    },
    "files.autoSave": "afterDelay",
    "files.eol": "\n",
    "files.hotExit": "off",
    "geminicodeassist.project": "amazing-fort-fpmhn",
    "git.confirmSync": false,
    "git.defaultBranchName": "master",
    "git.enableSmartCommit": true,
    "git.inputValidationSubjectLength": 255,
    "git.openRepositoryInParentFolders": "always",
    "github.copilot.enable": {
        "*": false,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "html.format.wrapAttributes": "preserve",
    "html.format.wrapLineLength": 99,
    "isort.args": [
        "--profile",
        "black"
    ],
    "java.code.generators.fluentMethodPrefix": "with",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.dependency.packagePresentation": "hierarchical",
    "java.import.gradle.version": "7.4.2",
    "java.inlayHints.parameterNames.enabled": "all",
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.saveActions.organizeImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "markdown.extension.orderedList.autoRenumber": false,
    "markdown.extension.print.absoluteImgPath": false,
    "markdown.extension.print.onFileSave": false,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.importFormat": "relative",
    "python.analysis.typeCheckingMode": "basic",
    "redhat.telemetry.enabled": true,
    "rsp-ui.enableStartServerOnActivation": [
        {
            "id": "redhat.vscode-community-server-connector",
            "name": "Community Server Connector",
            "startOnActivation": false
        }
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "sonarlint.automaticAnalysis": true,
    "svg.preview.mode": "svg",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "todo-tree.general.tags": [
        "EVAL",
        "TODO"
    ],
    "todo-tree.regex.regex": "\\|\\s($TAGS)\\s\\|\\s",
    "todo-tree.tree.labelFormat": "| ${tag} ${after}",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.restoreWindows": "none",
    "window.title": "${rootNameShort} :: ${activeEditorShort}",
    "workbench.colorTheme": "Dracula Theme",
    "workbench.editor.empty.hint": "hidden",
    "workbench.iconTheme": "bearded-icons",
    "workbench.panel.opensMaximized": "never",
    "workbench.startupEditor": "none",
    "xml.server.vmargs": "-Xmx128M",
    "zenMode.restore": false
}