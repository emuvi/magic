{
  "editor.tabSize": 2,
  "editor.fontSize": 18,
  "editor.lineHeight": 33,
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "editor.wrappingIndent": "deepIndent",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.fontWeight": "bold",
  "editor.fontFamily": "'Hack', 'monospace', monospace",
  "workbench.colorTheme": "Primer Light",
  "workbench.iconTheme": "bearded-icons",
  "workbench.startupEditor": "none",
  "window.restoreWindows": "none",
  "files.hotExit": "off",
  "cSpell.language": "en,pt,pt_BR",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "editor.rulers": [90],
  "prettier.printWidth": 90,
  "html.format.wrapLineLength": 90,
  "python.showStartPage": false,
  "cmake.configureOnOpen": false,
  "C_Cpp.updateChannel": "Insiders",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.semanticHighlighting.enabled": true,
  "java.format.settings.profile": "PointelStyle",
  "java.format.settings.url": "https://github.com/emuvi/dotfiles/raw/master/java-pointel-style.xml",
  "java.project.importOnFirstTimeStartup": "automatic",
  "redhat.telemetry.enabled": true,
  "gutterpreview.showImagePreviewOnGutter": false,
  "docker.showStartPage": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[py]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[php]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "kokororin.vscode-phpfmt"
  },
  "[phtml]": {
    "editor.defaultFormatter": "kokororin.vscode-phpfmt"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[dart]": {
    "editor.rulers": [90],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "errorLens.errorColor": "rgba(240,0,0,0.1)",
  "errorLens.warningColor": "rgba(180,180,0,0.1)",
  "errorLens.hintColor": "rgba(20,140,140,0.1)",
  "errorLens.infoColor": "rgba(0,200,0,0.1)",
  "workbench.colorCustomizations": {
    "editor.background": "#fffdf5",
    "tab.activeBackground": "#3EE2B9",
    "tab.inactiveBackground": "#DAE9E5"
  },
  "editor.tokenColorCustomizations": {
    "keywords": "#EE3333",
    "functions": "#3333EE",
    "variables": "#555555",
    "numbers": "#EE7755",
    "strings": "#557755",
    "types": "#EE33EE",
    "comments": "#A5A5A5",
    "textMateRules": [
      {
        "scope": "punctuation",
        "settings": {
          "foreground": "#777777"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#EE33EE"
        }
      },
      {
        "scope": "storage.type.primitive",
        "settings": {
          "foreground": "#EE33EE"
        }
      },
      {
        "scope": "meta.class storage.type",
        "settings": {
          "foreground": "#EE33EE"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#EE3333"
        }
      }
    ]
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.html": "default"
  },
  "cSpell.userWords": [
    "pointel",
    "pointto",
    "varchar"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  }
}
